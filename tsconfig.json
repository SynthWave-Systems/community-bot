{
	"compilerOptions": {
		"target": "ES2020",
		"module": "Node16",
		"lib": ["ES2020"],
		"outDir": "./dist",
		"rootDir": "./src",
		"types": ["discord.js", "dotenv-safe"],

		"strict": true,
		"esModuleInterop": true,

		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,

		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	}
}
